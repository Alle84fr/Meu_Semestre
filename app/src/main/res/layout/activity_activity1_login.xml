<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_1_login">

    <ImageView
        android:id="@+id/img_ini_preta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/img_inic_a_fund_peto"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="178dp" />

    <ImageView
        android:id="@+id/img_ini_log"
        android:layout_width="184dp"
        android:layout_height="185dp"
        app:srcCompat="@drawable/img_inic_b_logo"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="81dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_ini_login"
        android:layout_width="138dp"
        android:layout_height="46dp"
        android:background="@drawable/btn_inic_c_bot_top"
        android:text="Login"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="342dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_ini_planos"
        android:layout_width="138dp"
        android:layout_height="46dp"
        android:background="@drawable/btn_inic_c_bot_top"
        android:text="Planos"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="487dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_ini_cadastro"
        android:layout_width="138dp"
        android:layout_height="46dp"
        android:background="@drawable/btn_inic_c_bot_top"
        android:text="Cadastro"
        android:textColor="#000000"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="414dp" />

</androidx.constraintlayout.widget.ConstraintLayout>